<!-- finding if user wants books or articles.  default pattern -->
<category>
	<pattern>I WANT MORE INFORMATION ON *</pattern>
	<template><think><set name="topic">booksorarticles</set><set name="search"><star /></set></think>Did you want articles or books on <star />? Say: "articles" or "books".</template>
